                    <ContextMenu>
                        <MenuItem Header="Help"
                                    Command="self:CustomCommands.HelpClick"
                                    CommandParameter="{Binding ElementName=txtCorrelationLengthYDir}" />
                    </ContextMenu>
                </Label.ContextMenu>
