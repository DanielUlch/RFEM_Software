        <StackPanel Orientation="Horizontal">
            <Label Content="Covariance function:"
                   ToolTip="{StaticResource DefaultToolTip}">
                <Label.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Help"
                                    Command="self:CustomCommands.HelpClick"
