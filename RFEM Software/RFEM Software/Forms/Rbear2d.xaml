                                    Command="self:CustomCommands.HelpClick"
                                    CommandParameter="{Binding ElementName=txtMaxNumIter}" />
                    </ContextMenu>
                </TextBox.ContextMenu>
            </TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
