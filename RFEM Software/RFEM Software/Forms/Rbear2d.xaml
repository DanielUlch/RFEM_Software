                    </ContextMenu>
                </Label.ContextMenu>
            </Label>
            <TextBox Name="txtMaxNumIter"
                     ToolTip="{StaticResource DefaultToolTip}"
                     Width="50">
                <TextBox.ContextMenu>
                    <ContextMenu>
