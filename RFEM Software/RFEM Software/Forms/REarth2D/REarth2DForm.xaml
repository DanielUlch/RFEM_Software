                                <cc:RFEMToolTip Title="Pressure Coefficient Lower Bound"
                                                ToolTipMessage="The lower bound of the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                        <Label Name="lbPressureCoefficientUpperBound"
                               Tag="REarthPressureCoefficientDist"
                               Content="Upper bound:">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Upper Bound"
                                                ToolTipMessage="The upper bound of the pressure coefficient distribution."/>
                            </Label.ToolTip>
                        </Label>

                        <TextBox Name="txtPressureCoefficientUpperBound"
                                 Tag="REarthPressureCoefficientDist"
                                 Text="{Binding Path=PressureCoefficient.UpperBound, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True, Converter={StaticResource DoubleConverter}}">
                            <TextBox.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Upper Bound"
                                                ToolTipMessage="The upper bound of the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                    </StackPanel>

                    <StackPanel Orientation="Horizontal">

                        <Label Name="lbPressureCoefficientLocation"
                               Tag="REarthPressureCoefficientDist"
                               Content="Location:">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Location"
                                                ToolTipMessage="The location parameter of the pressure coefficient distribution."/>
                            </Label.ToolTip>
                        </Label>

                        <TextBox Name="txtPressureCoefficientLocation"
                                 Tag="REarthPressureCoefficientDist"
                                 Text="{Binding Path=PressureCoefficient.Location, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors =True, Converter={StaticResource DoubleConverter}}">
                            <TextBox.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Location"
                                                ToolTipMessage="The location parameter of the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                        <Label Name="lbPressureCoefficientScale"
                               Tag="REarthPressureCoefficientDist"
                               Content="Scale:">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Scale"
                                                ToolTipMessage="The scale parameter of the pressure coefficient distribution."/>
                            </Label.ToolTip>
                        </Label>

                        <TextBox Name="txtPressureCoefficientScale"
                                 Tag="REarthPressureCoefficientDist"
                                 Text="{Binding Path=PressureCoefficient.Scale, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True, Converter={StaticResource DoubleConverter}}">
                            <TextBox.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Scale"
                                                ToolTipMessage="The scale parameter of the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                    </StackPanel>
                    
                </StackPanel>

                <StackPanel Orientation="Vertical">
                    <StackPanel.Style>
                        <Style TargetType="{x:Type StackPanel}">
                            <Setter Property="Visibility" Value="Visible" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=cboPressureCoefficientDist, Path=SelectedValue}" Value="{x:Static infra:REarthDistributions.Deterministic}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=cboPressureCoefficientDist, Path=SelectedValue}" Value="{x:Static infra:REarthDistributions.Normal}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=cboPressureCoefficientDist, Path=SelectedValue}" Value="{x:Static infra:REarthDistributions.Lognormal}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=cboPressureCoefficientDist, Path=SelectedValue}" Value="{x:Static infra:REarthDistributions.Bounded}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </StackPanel.Style>

                    <StackPanel Orientation="Horizontal" >

                        <Label Name="lbPressureCoefficientIntercept"
                               Tag="REarthPressureCoefficientDist"
                               Content="Intercept a:">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Intercept"
                                                ToolTipMessage="The intercept parameter for the pressure coefficient distribution."/>
                            </Label.ToolTip>
                        </Label>

                        <TextBox Name="txtPressureCoefficientIntercept"
                                 Tag="REarthPressureCoefficientDist"
                                 Text="{Binding Path=PressureCoefficient.Intercept, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True, Converter={StaticResource DoubleConverter}}">
                            <TextBox.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Intercept"
                                                ToolTipMessage="The intercept parameter for the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                        <Label Name="lbPressureCoefficientSlope"
                               Tag="REarthPressureCoefficientDist"
                               Content="Slope b:">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Slope"
                                                ToolTipMessage="The slope parameter for the pressure coefficient distribution."/>
                            </Label.ToolTip>
                        </Label>

                        <TextBox Name="txtPressureCoefficientSlope"
                                 Tag="REarthPressureCoefficientDist"
                                 Text="{Binding Path=PressureCoefficient.Slope, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True, Converter={StaticResource DoubleConverter}}">
                            <TextBox.ToolTip>
                                <cc:RFEMToolTip Title="Pressure Coefficient Slope"
                                                ToolTipMessage="The slope parameter for the pressure coefficient distribution."/>
                            </TextBox.ToolTip>
                        </TextBox>

                    </StackPanel>

                    <StackPanel Orientation="Horizontal">

                        <Label Name="lbPressureCoefficientPhiFunc"
                               Tag="REarthPressureCoefficientDist"
                               Content="f(phi):">
                            <Label.ToolTip>
                                <cc:RFEMToolTip Title="Friction Angle Function"
                                                ToolTipMessage="The function of phi used in: property = a + b*f(phi)"/>
                            </Label.ToolTip>
                        </Label>

                        <ComboBox Name="cboPressureCoefficientPhiFunc"
                                  Tag="REarthPressureCoefficientDist"
                                  ItemsSource="{Binding Source={StaticResource PhiFuncEnum}}"
                                  SelectedValue="{Binding Path=PressureCoefficient.PhiFunc, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource UIStringConverter}}"/>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                            <ComboBox.ToolTip>
                                <cc:RFEMToolTip Title="Friction Angle Function"
                                                ToolTipMessage="The function of phi used in: property = a + b*f(phi)"/>
                            </ComboBox.ToolTip>
                        </ComboBox>

                    </StackPanel>

                </StackPanel>

            </StackPanel>

            <Label Name="lbCorrelationMatrix"
                   Tag="REarthCorrelationMatrix"
                   Content="Soil property correlation matrix elements:">
                <Label.ToolTip>
                    <cc:RFEMToolTip Title="Correlation Matrix"
                                    ToolTipMessage="The correlation coefficients, acting between each random soil property, can be defined in the following table."/>
                </Label.ToolTip>
            </Label>

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <DataGrid Name="dgCorrelationMatrix"
                          Tag="REarthCorrelationMatrix"
                          AutoGenerateColumns="True"
                          Grid.Column="0"
                          AutoGeneratingColumn="dgCorrelationMatrix_AutoGeneratingColumn"
                          CanUserAddRows="False"
                          CanUserDeleteRows="False"
                          CanUserReorderColumns="False"
                          CanUserSortColumns="False"
                          LoadingRow="dgCorrelationMatrix_LoadingRow"
                          Width="auto"/>
            </Grid>
            
        </StackPanel> 
        
    </Grid>
</UserControl>
